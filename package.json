{
  "name": "gatsby-starter-typescript",
  "description": "Gatsby TypeScript starter",
  "version": "1.0.0",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "dependencies": {
    "@types/node": "^10.11.2",
    "@types/react": "16.4.14",
    "@types/react-dom": "16.0.8",
    "babel-plugin-styled-components": "^1.8.0",
    "gatsby": "^2.0.19",
    "gatsby-link": "^2.0.4",
    "gatsby-plugin-react-helmet": "^3.0.0",
    "gatsby-plugin-styled-components": "^3.0.0",
    "gatsby-plugin-typescript": "^2.0.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-fontawesome": "^1.6.1",
    "react-helmet": "^5.2.0",
    "styled-components": "^3.4.9"
  },
  "keywords": [
    "gatsby, typescript"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --parser typescript --trailing-comma all --single-quote --write \"src/**/*.ts*\"",
    "generate:stateless": "node ./scripts/stateless.js",
    "generate:stateful": "node ./scripts/stateful.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public"
  },
  "devDependencies": {
    "@types/react-helmet": "^5.0.7",
    "genmite": "^1.2.0",
    "gh-pages": "^2.0.1",
    "husky": "^1.1.1",
    "lint-staged": "^7.3.0",
    "prettier": "^1.14.3"
  },
  "contributors": [],
  "lint-staged": {
    "*.{ts, tsx}": [
      "npm run format",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
